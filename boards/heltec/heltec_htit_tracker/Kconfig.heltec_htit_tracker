config BOARD_HELTEC_HTIT_TRACKER
	select SOC_ESP32S3_FN8
	select SOC_ESP32S3_PROCPU if BOARD_HELTEC_HTIT_TRACKER_ESP32_PROCPU
	select SOC_ESP32S3_APPCPU if BOARD_HELTEC_HTIT_TRACKER_ESP32_APPCPU
