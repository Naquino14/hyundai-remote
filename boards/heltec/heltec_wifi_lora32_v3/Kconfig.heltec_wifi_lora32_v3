config BOARD_HELTEC_WIFI_LORA32_V3
	select SOC_ESP32_D0WD_V3
	select SOC_ESP32S3_PROCPU if BOARD_HELTEC_WIFI_LORA32_V3_ESP32_PROCPU
	select SOC_ESP32S3_APPCPU if BOARD_HELTEC_WIFI_LORA32_V3_ESP32_APPCPU
